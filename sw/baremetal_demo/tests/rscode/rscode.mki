TEST_LIBS = -L $(GCC_PATH)/riscv64-unknown-elf/lib/rv32imac/ilp32 -L $(GCC_PATH)/lib/gcc/riscv64-unknown-elf/10.2.0/rv32imac/ilp32 -lc -lgcc -lnosys 
